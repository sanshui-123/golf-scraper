    <key>StartInterval</key>
    <integer>300</integer>
    
    <key>ThrottleInterval</key>
    <integer>60</integer>
    
    <key>StandardOutPath</key>
    <string>/Users/sanshui/Desktop/cursor/logs/daemon.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/sanshui/Desktop/cursor/logs/daemon-error.log</string>
    
    <key>UserName</key>
    <string>sanshui</string>
</dict>
</plist>