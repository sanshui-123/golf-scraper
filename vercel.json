{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "golf_content/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/golf_content/(.*)",
      "dest": "/golf_content/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}