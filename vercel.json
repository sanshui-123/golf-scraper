{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/", "destination": "/api/home" },
    { "source": "/api/test-api", "destination": "/api/test-api" },
    { "source": "/(.*)", "destination": "/api/index" }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/home.js": {
      "maxDuration": 30
    },
    "api/test-api.js": {
      "maxDuration": 10
    }
  }
}