                return false;
            }
            
            console.log(`âœ… åå°æ¨¡å¼æµ‹è¯•é€šè¿‡`);
            return true;
            
        } catch (error) {
            console.log(`âŒ åå°æ¨¡å¼æµ‹è¯•å¤±è´¥: ${error.message}`);
            return false;
        }
    }
}

